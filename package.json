{
  "name": "phaser-webpack-typescript",
  "version": "1.1.0",
  "description": "Boilerplate project to use phaser with webpack",
  "main": "greeter.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run && webpack --config ./webpack.config.js --progress --colors",
    "build:dev": "NODE_ENV=develop npm run build",
    "build:production": "NODE_ENV=production npm run build",
    "build:watch": "npm run build --watch",
    "serve": "webpack serve",
    "start": "npm run server"
  },
  "author": "Henrique Clementino",
  "license": "ISC",
  "devDependencies": {
    "@webpack-cli/serve": "^1.5.2",
    "ts-loader": "^6.2.2",
    "typescript": "^3.9.10",
    "webpack": "^5.55.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.3.0"
  },
  "dependencies": {
    "phaser": "^3.55.2"
  }
}
